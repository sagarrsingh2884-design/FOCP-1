QUE 1 TOGGLE STRING
#include <stdio.h>


int main() {
    char s[1000];
    
    scanf("%s", s);

    
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] >= 'A' && s[i] <= 'Z') {
            s[i] = s[i] + ('a' - 'A');   
        } else if (s[i] >= 'a' && s[i] <= 'z') {
            s[i] = s[i] - ('a' - 'A');   
        }
    }


    printf("%s", s);

    return 0;
}

QUE 2 cost of balloon
#include <stdio.h>

int main() {
    int t; 
    scanf("%d", &t);  

    while (t--) {
        int green, purple;
        scanf("%d %d", &green, &purple); 

        int n;
        scanf("%d", &n); 

        int p1 = 0, p2 = 0; 

        for (int i = 0; i < n; i++) {
            int a, b; 
            scanf("%d %d", &a, &b); 
    
            p1 += a;
            p2 += b;
        }

        
        int cost1 = p1 * green + p2 * purple;

        
        int cost2 = p1 * purple + p2 * green;

        \
        if (cost1 < cost2)
            printf("%d\n", cost1);
        else
            printf("%d\n", cost2);
    }

    return 0;
}
QUE 3 zoos
#include <stdio.h>

int main() {
    char s[1000];
    scanf("%s", s);

    int countZ = 0, countO = 0;


    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == 'z' || s[i] == 'Z')
            countZ++;
        else if (s[i] == 'o' || s[i] == 'O')
            countO++;
    }


    if (countO == 2 * countZ)
        printf("Yes\n");
    else
        printf("No\n");

    return 0;               
}
QUE 4 roy and profile pic 
#include <stdio.h>

int main() {
    int L, N;
    scanf("%d", &L);  
    scanf("%d", &N);  

    while (N--) {
        int W, H;
        scanf("%d %d", &W, &H);

        if (W < L || H < L) {
            printf("UPLOAD ANOTHER\n");
        }
      
        else {
            if (W == H) {
                printf("ACCEPTED\n");  
            } else {
                printf("CROP IT\n");  
            }
        }
    }

    return 0;
}
